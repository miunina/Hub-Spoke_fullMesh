
##########################################################################
alpha_switch :
##########################################################################
##########################################################################VLANS
configure terminal 
vlan 10
name VLAN_10
end

configure terminal 
vlan 99
name native
end


configure terminal 
vlan 30
name VLAN_30
end

configure terminal 
vlan 101
name VLAN_101
exit
##########################################################################TRUNK
interface g0/0
switchport trunk allowed vlan 10,30,99,101
switchport trunk native vlan 99
switchport trunk encapsulation dot1q
switchport mode trunk
vlan dot1q tag native

interface g0/1
switchport trunk allowed vlan 10,30,99,101
switchport trunk native vlan 99
switchport trunk encapsulation dot1q
switchport mode trunk
vlan dot1q tag native
##########################################################################ACCESS
conf t
interface g0/2
switchport mode access
switchport access vlan 101
end
conf t
interface g0/3
switchport mode access
switchport access vlan 101
end


