
##########################################################################
#alpha_switch :
##########################################################################
##########################################################################VLANS
configure terminal 
vlan 40
name vlan10
end

configure terminal 
vlan 99
name native
end

configure terminal 
vlan 101
name vlan101
end
##########################################################################TRUNK
interface g0/0
switchport trunk encapsulation dot1q
switchport mode trunk
##########################################################################ACCESS
interface g0/2
switchport mode access
switchport access vlan 101
end
interface g0/3
switchport mode access
switchport access vlan 101
end

##########################################################################40
interface g1/0
switchport mode access
switchport access vlan 40
exit
interface g1/1 
switchport mode access
switchport access vlan 40
exit
interface g1/2 
switchport mode access
switchport access vlan 40
exit
interface g1/3 
switchport mode access
switchport access vlan 40
exit
    
   
  