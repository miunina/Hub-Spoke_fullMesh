
##########################################################################
alpha_switch :
##########################################################################
#####################################VLANS#####################################
configure terminal 
vlan 10
name VLAN_10
end

configure terminal 
vlan 101
name VLAN_101
exit

 
vlan 99
name native
exit



#####################################TRUNK#####################################
switchport trunk allowed vlan 10,101,99
switchport trunk native vlan 99

switchport nonegotiate

interface Gi0/0
switchport trunk encapsulation dot1q

switchport mode trunk

no sh
####################################ACCESS 101#####################################
interface g0/1
switchport mode access
switchport access vlan 101
no sh
exit

interface g0/2
switchport mode access
switchport access vlan 101
no sh
exit

interface g0/3
switchport mode access
switchport access vlan 101
no sh
exit

################################ACCESS 10############################################
interface g1/0
switchport mode access
switchport access vlan 10
exit
interface g1/1 
switchport mode access
switchport access vlan 10
exit
interface g1/2 
switchport mode access
switchport access vlan 10
exit
interface g1/3 
switchport mode access
switchport access vlan 10
exit
    
vlan dot1q tag native
  