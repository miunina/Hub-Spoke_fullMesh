#######################################################################################################
#CONFIG INTERFACE NO sh DHCP
#######################################################################################################
conf t
int g0/0 
no sh

int g0/1
no sh

int g0/2
no sh
#######################################################################################################
int g0/1.40
encapsulation dot1q 40
ip address 172.150.2.1 255.255.255.192
exit
int g0/1.30
encapsulation dot1q 30
ip address 172.150.2.65 255.255.255.224
exit
int g0/1.101
encapsulation dot1q 101
ip address 172.150.2.97 255.255.255.240
exit
int g0/1.99
encapsulation dot1q 99
ip address 172.150.2.113 255.255.255.240
exit
#######################################################################################################
ip dhcp excluded-address 172.150.2.1 172.150.2.65
ip dhcp excluded-address 172.150.2.65
ip dhcp excluded-address 172.150.2.97 172.150.2.113
ip dhcp excluded-address 172.150.2.113

#######################################################################################################

ip dhcp pool VLAN_40
network 172.150.2.0 255.255.255.192
default-router 172.150.2.1
exit 


ip dhcp pool VLAN_30
network 172.150.2.64 255.255.255.224
default-router 172.150.2.65
exit 

ip dhcp pool VLAN_101
network 172.150.2.96 255.255.255.240
default-router 172.150.2.97
exit 

ip dhcp pool native
network 172.150.2.112 255.255.255.240
default-router 172.150.2.113
exit 

