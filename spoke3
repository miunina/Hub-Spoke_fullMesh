########################################################################################################CONFIGURE INTERFACES VLANs and WAN 
#CONFIG LAN , WAN , VLANs 
#######################################################################################################
config system interface
                edit port2
                    set alias wan1
                    set ip  192.162.12.2 255.255.255.0
                    
                next
                edit port4
                    set alias wan2
                    set ip 12.1.2.2 255.255.255.0
                next
end
#VLANS:
config system interface
               edit VLAN_10
                    set alias vlan10
                    set vdom root
                    set interface port3
                    set ip 172.150.4.1 255.255.255.192
                    set type vlan
                    set vlanid 10
                next
                edit VLAN_30 
                    set alias vlan30
                    set vdom root
                    set interface port3
                    set ip 172.150.4.65 255.255.255.224
                    set type vlan
                    set vlanid 30
                next
                edit VLAN_101
                    set alias vlan101
                    set vdom root
                    set interface port3
                    set ip 172.150.4.97 255.255.255.240
                    set type vlan
                    set vlanid 101
                next
                edit VLAN_99
                    set alias native
                    set vdom root
                    set interface port3
                    set ip 172.150.4.113 255.255.255.240
                    set type vlan
                    set vlanid 99
                next
end 
#######################################################################################################
 #CONNECT TO INTERNET GUI
#######################################################################################################
     config system interface
        edit port10
            set mode static 
            set ip 192.168.1.100 255.255.255.0
                        #22.1.1.1 
            set allowaccess ping https http ssh
        next
        end
#######################################################################################################
#CONFIGURE TOUES STATICS
#######################################################################################################
config router static
    edit 1
                    set gateway 12.1.2.1
                    set device port4
                    set distance 15        
    next
    edit 2
                    set gateway 192.162.12.1  
                    set device port2
    next
end 
#######################################################################################################
#CONFIGURE LAN ZONE 
#######################################################################################################
  config system zone
        edit teta_lan
            set interface internal VLAN_101
            set interface internal VLAN_40
            set interface internal VLAN_99
            set intrazone allow
        next
end
#######################################################################################################  
#CONFIG DHCP
#######################################################################################################
pool vlan-10 172.150.4.1 172.150.4.62   192
pool vlan-30 172.150.4.65 172.150.4.94    224
pool vlan-101 172.150.4.97 172.150.4.110 240
pool native 172.150.2.113 172.150.2.126 240